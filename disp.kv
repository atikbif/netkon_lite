<AnalogData>:
    orientation:"vertical"
    

<AnalogDataView>:
    orientation:"horizontal"
    Label:
        id: name
        text: "adcqwe"
    Label:
        id: value
        text: "0"
    Label:
        id: measure_unit
        text: "-"

<ObjView>:
    orientation:"vertical"
    obj_name:""
    obj_time:""
    Label:
        markup:True
        text:root.obj_name
        size_hint_y: None
        height: "50dp"
    Label:
        markup:True
        size_hint_y: None
        height: "50dp"
        text:root.obj_time
    Button:
        id: list_button
        size_hint_y: None
        height: "50dp"
        text: ""
        on_press: app.root.show_obj_list()

<LocationButton>:
    on_press: app.root.show_current_object(self.text)
 